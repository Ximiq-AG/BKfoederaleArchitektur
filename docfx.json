{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "architecture/**.md",
          "architecture/**/toc.yml",
          "sales/**.md",
          "sales/**/toc.yml",
          "cpsv/**.md",
          "cpsv/**/toc.yml",
          "toc.yml",
          "*.md",
          "TestFolder/**.md",
          "TestFolder/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "templates/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Kollaboratives Architekturmanagement",
      "_appFooter": "Ximiq AG",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/Ximiq-AG/BKfoederaleArchitektur",
        "branch": "main"
      }
    },
    "fileMetadata": {
      "_gitContribute": {
        "TestFolder/**": {
          "repo": "https://github.com/JNGNGY/DocFX_Test",
          "branch": "main"
        }
      }
    },
    "template": [
      "default",
      "templates"
    ]
  }
}
